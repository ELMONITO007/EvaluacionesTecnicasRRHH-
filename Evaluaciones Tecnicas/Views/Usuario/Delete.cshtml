@model Entities.Usuarios

@{
    ViewBag.Title = "Delete";
    string layout = "~/Views/Shared/" + Session["Layout"] + ".cshtml";
    Layout = layout;
}
}


<link href="~/Content/modern-business.css" rel="stylesheet" />

<div>
    <h3>¿Esta seguro de eliminar <i> "@Html.DisplayFor(model => model.Email)"</i>?</h3>
    <hr />




    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-outline-info" />
            <input type="button" class="btn btn-outline-primary " style="float:right" value="Volver" onclick="location.href='@Url.Action("Index", "Usuario")'" />
        </div>
    }
</div>
@section scripts
{
}