@model Entities.Jefatura

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

    <link href="~/Content/modern-business.css" rel="stylesheet" />
    <div>
        <h3>¿Esta seguro de eliminar la Empresa  <i>"@Html.DisplayFor(model => model.jefatura)"</i>?</h3>


        <br />

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <input type="submit" value="Eliminar" class="BotonVerde" />
        <input type="button" class="BotonVerde " style="float:right" value="Volver" onclick="location.href='@Url.Action("Index", "Jefatura")'" />
    </div>
        }
    </div>
