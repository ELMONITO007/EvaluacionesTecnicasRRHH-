@model Entities.Empresa

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}
<link href="~/Content/modern-business.css" rel="stylesheet" />

<div>
    <h3>¿Esta seguro de eliminar la Empresa  <i>"@Html.DisplayFor(model => model.empresa)"</i>?</h3>

    <br />

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()


<div class="form-actions no-color">
    <input type="submit" value="Eliminar" class="btn btn-outline-info" />
  
    <input type="button" class="btn btn-outline-primary " style="float:right" value="Volver" onclick="location.href='@Url.Action("Index", "direccion")'" />
</div>

    }
</div>